script Research_Papers {
    lockall
    giveitem(ITEM_RARE_CANDY)
    setflag(FLAG_Research_Papers)
    removeitem(1)
    setvar(VAR_WHIMLEAF_INTRO_STATE,3)
    releaseall
}

